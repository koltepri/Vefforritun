{
  "name": "beta/link",
  "selectors": [
    ".Link",
    ".Link:hover",
    ".Link:focus",
    ".Link:focus-visible",
    ".Link--underline",
    ".Link--primary",
    ".Link--primary:hover",
    ".Link--secondary",
    ".Link--secondary:hover",
    ".Link--muted",
    ".Link--muted:hover",
    ".Link--onHover:hover",
    ":is(.Link--secondary,.Link--primary,.Link--muted):hover [class*=color-fg]"
  ]
}