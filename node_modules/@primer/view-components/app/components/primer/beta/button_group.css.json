{
  "name": "beta/button_group",
  "selectors": [
    ".ButtonGroup",
    ".ButtonGroup .Button",
    ":is(.ButtonGroup .Button):active",
    ":is(.ButtonGroup .Button):focus",
    ":is(.ButtonGroup .Button):hover",
    ".ButtonGroup>:first-child .Button",
    ".ButtonGroup>:last-child .Button"
  ]
}